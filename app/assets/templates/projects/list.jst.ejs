<div id="project_list" class="row">
	<div clas="col-xs-2 col-xs-offset-1">
		<button id="new_project" style= "margin-top: 15px; margin-left: 15px;" class="btn btn-sherpa">Start New Project</button>
	</div>
		<%projects.forEach(function(project){ %>
		<div class="col-xs-2 col-xs-offset-1">
			<a class="sherp-links project-square" href=<%= "#/projects/" + project.id + "/todos"%>>
				<div class="sherpa-square" data-id="<%=project.id%>">
					<p align="center" style="margin-top: 15px; padding-top: 10px"><%= project.escape('title') %>
					</p>
					<p align="center" style="font-size: 11px; padding-left: 5px; padding-right: 5px;"><%=project.escape('description')%>
					</p>
				</div>
				</a>
			</div>
		<%});%>

</div>