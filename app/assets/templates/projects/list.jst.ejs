<div id="project_list">
	<ul>
		<%projects.forEach(function(project){ %>
			<div class="sherpa-square">
				<a href=<%= "#/projects/" + project.id%>>
					<%= project.escape('title') %>
				</a>
			</div>
			<br>
		<%});%>
	</ul>
	<button id="new_project" class="btn btn-sherpa">Start New Project</button>
</div>