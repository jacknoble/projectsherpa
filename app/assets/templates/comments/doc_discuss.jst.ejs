<div class="modal-dialog">
  <div class="modal-content" style="width: 850px">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
			<div>
				<object data="<%=doc.escape('file')%>" type="<%=doc.get('file_type')%>" width="800" height="500">
					alt : <a href="<%=doc.escape('file')%>"> <%=doc.escape('file')%></a>
				</object>
			</div>
			</div>
    </div>
    <div class="modal-body">
			<div id="comments">
			</div>
			<button
				class="btn btn-sm btn-sherpa"
				id="reply">
				Reply
			</button>
    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-sherpa" data-dismiss="modal">Close</button>
    </div>
  </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->
