<span id="list_edit_form" data-id="<%=list.escape('id')%>">
	<form class="form-inline">
		<input
			class="input-sm subtle-form-input"
			id="todo_list_title"
			type="text"
			name="todo_list[title]"
			value= "<%=list.escape('title')%>"
			data-id="<%=list.escape('id')%>"
		>
		<input
			class="btn btn-xs btn-sherpa"
			type="submit"
			value="OK"
			id="update_list"
			data-id="<%=list.escape('id')%>">

		<button
			class="btn btn-xs btn-sherpa"
			id="cancel_list_edit"
			data-id="<%=list.escape('id')%>">X
		</button>
		<button class="btn-sherpa" id="delete" data-id="<%=list.id%>">
			<span class="glyphicon glyphicon-trash"></span>
		</button>
	</form>
</span>